# Phase 3 Test: Functions

fn fib(n) {
  if (n < 2) return n;
  return fib(n - 1) + fib(n - 2); 
}

let start = 10; 
# Note: clock() is not implemented yet.
let f = fib(start);
print(f); # Expect 55

