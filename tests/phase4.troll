# Phase 4 Test: Deep Learning Features

# Array Literal
let a = [1, 2, 3];
print(a); # Expect [1.000000, 2.000000, 3.000000]

# Matrix
let A = [
  [1, 2],
  [3, 4]
];
print(A); 
# Expect [[1.0, 2.0], [3.0, 4.0]]

let B = [
  [0, 1],
  [1, 0]
];

# MatMul
let C = A @ B;
print(C);
# Expect:
# [[1*0 + 2*1, 1*1 + 2*0],
#  [3*0 + 4*1, 3*1 + 4*0]]
# = [[2, 1], [4, 3]]
